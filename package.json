{
  "name": "react_mp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "husky": "^0.13.3",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "react-scripts": "0.9.5",
    "svg-sprite": "^1.3.6"
  },
  "scripts": {
    "start": "npm run build-svg && npm-run-all -p watch-css start-js",
    "build": "npm run build-svg && npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start-js": "react-scripts start",
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "lint": "eslint ./src",
    "precommit": "npm run lint",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "build-svg": "node tools/svg-sprite.js"
  }
}
